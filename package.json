{
  "name": "tsed-example-vite",
  "version": "1.0.0",
  "description": "POC showing Ts.ED integrating with ViteJS",
  "main": "index.js",
  "repository": "https://github.com/tsedio/tsed-example-vite",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn run tsc",
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "test:unit": "cross-env NODE_ENV=test jest",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w"
  },
  "author": "",
  "dependencies": {
    "@tsed/common": "^6.49.3",
    "@tsed/core": "^6.49.3",
    "@tsed/di": "^6.49.3",
    "@tsed/exceptions": "^6.49.3",
    "@tsed/logger": "^5.10.0",
    "@tsed/platform-express": "^6.49.3",
    "@tsed/schema": "^6.49.3",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "envalid": "^7.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "method-override": "^3.0.0",
    "pullstate": "^1.22.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.34.0",
    "serve-static": "^1.13.1",
    "ts-json-properties": "^2.0.3",
    "vite": "^2.3.2",
    "vite-plugin-ssr": "0.1.0-beta.44",
    "vite-plugin-windicss": "^0.15.10",
    "windicss": "^3.0.9"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "^3.0.6",
    "@tsed/cli-plugin-jest": "^3.0.6",
    "@tsed/cli-plugin-typeorm": "^3.0.6",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/http-proxy": "^1.16.2",
    "@types/jest": "^26.0.23",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.17.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/request-promise": "^4.1.41",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "lint-staged": "^11.0.0",
    "nodemon": "2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "packages/client/**/*.{js,vue}": [
      "cd packages/client && yarn lint:fix && yarn prettier",
      "git add"
    ],
    "packages/server/**/*.{ts}": [
      "cd packages/server && yarn lint:fix && yarn prettier",
      "git add"
    ]
  }
}
